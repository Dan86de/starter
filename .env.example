# Database configuration based on docker-compose.yaml
# These are example values matching the Docker Compose setup

POSTGRES_DB=starter_db
POSTGRES_USER=starter_user
POSTGRES_PASSWORD=starter_pass
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

# Combined DATABASE_URL for ORMs like Prisma, Sequelize, etc.
DATABASE_URL=postgresql://${POSTGRES_USER}:${POSTGRES_PASSWORD}@${POSTGRES_HOST}:${POSTGRES_PORT}/${POSTGRES_DB}
